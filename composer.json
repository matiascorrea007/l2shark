{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.5.9",
        "laravel/framework": "5.2.*",
        "laravelcollective/html": "5.2.*",
        "mercadopago/sdk": "0.5.2",
        "barryvdh/laravel-debugbar": "^2.2",
        "gloudemans/shoppingcart": "^1.3",
        "intervention/image": "2.*",
        "kim/activity": "^1.1",
        "kryptonit3/counter": "5.2.*",
        "maatwebsite/excel": "~2.1.0",
        "uxweb/sweet-alert": "~1.1",
        "pqb/filemanager-laravel": "2.*",
        "unisharp/laravel-filemanager": "1.6.4",
        "unisharp/laravel-ckeditor": "^4.5",
        "guzzlehttp/guzzle": "~6.0",
        "monolog/monolog": "^1.21",
        "arcanedev/log-viewer": "~3.0",
        "milon/barcode": "^5.3",
        "laracasts/flash": "^3.0",
        "embed/embed": "^3.0",
        "anouar/paypalpayment": "~1.0",
        "mews/captcha": "^2.1",
        "whossun/laravel-toastr": "^1.0",
        "greggilbert/recaptcha": "dev-master"

    

    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~4.0",
        "symfony/css-selector": "2.8.*|3.0.*",
        "symfony/dom-crawler": "2.8.*|3.0.*",
        "barryvdh/laravel-dompdf": "0.6.*",
        "ignasbernotas/laravel-model-generator": "^1.2"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "Soft\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
